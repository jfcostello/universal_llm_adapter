{
  "id": "openai-responses",
  "compat": "openai-responses",
  "endpoint": {
    "urlTemplate": "https://api.openai.com/v1/responses",
    "method": "POST",
    "headers": {
      "Authorization": "Bearer ${OPENAI_API_KEY}",
      "Content-Type": "application/json"
    }
  },
  "retryWords": [
    "rate",
    "overload",
    "limit",
    "exhausted",
    "Service Unavailable",
    "Too many requests"
  ]
}
