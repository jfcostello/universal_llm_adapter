{
  "retry": {
    "maxAttempts": 3,
    "baseDelayMs": 250,
    "multiplier": 2.0,
    "rateLimitDelays": [1, 1, 5, 5, 5, 15, 15, 16, 30, 31, 61, 5, 5, 51]
  },
  "tools": {
    "countdownEnabled": true,
    "finalPromptEnabled": true,
    "parallelExecution": false,
    "preserveResults": 3,
    "preserveReasoning": 3,
    "maxIterations": 10,
    "timeoutMs": 120000
  },
  "vector": {
    "topK": 5,
    "injectTemplate": "Relevant context:\n{{results}}",
    "resultFormat": "- {{payload.text}} (score: {{score}})",
    "batchSize": 10,
    "includePayload": true,
    "includeVector": false,
    "defaultCollection": "default"
  },
  "chunking": {
    "size": 500,
    "overlap": 50
  },
  "tokenEstimation": {
    "textDivisor": 4,
    "imageEstimate": 768,
    "toolResultDivisor": 6
  },
  "timeouts": {
    "mcpRequest": 30000,
    "llmHttp": 60000,
    "embeddingHttp": 60000,
    "loggerFlush": 2000
  },
  "paths": {
    "plugins": "./plugins"
  }
}
